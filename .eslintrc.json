{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
      "standard",
      "plugin:react/recommended",
      "plugin:@next/next/recommended",
      "next/core-web-vitals"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0
    }
}
